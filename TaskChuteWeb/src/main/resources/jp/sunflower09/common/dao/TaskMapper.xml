<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.sunflower09.common.dao.TaskMapper">

  <cache />

  <select id="selectByPrimaryKey" resultType="Task" parameterType="int">
    SELECT
      *
    FROM task
    WHERE id = #{id}
  </select>
